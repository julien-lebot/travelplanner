<div class="mdl-grid">
    <div class="mdl-cell--4-col"></div>
    <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp mdl-cell--4-col">
        <thead>
        <tr>
            <th class="mdl-data-table__cell--non-numeric">User</th>
            <th class="mdl-data-table__cell--non-numeric">Roles</th>
            <th></th>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="user in userData.users | filter:search" ng-click="goToUser(user.id)">
            <td class="mdl-data-table__cell--non-numeric">{{user.userName}}</td>
            <td class="mdl-data-table__cell--non-numeric">
                <div ng-repeat="role in user.roles">{{role}}</div></td>
        </tr>

        </tbody>
    </table>
    <div class="mdl-cell--4-col"></div>
</div>
<button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored" id="add-new" ng-click="addNewUser()">
    <i class="material-icons">add</i>
</button>
